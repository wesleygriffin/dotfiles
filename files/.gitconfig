[alias]
    a = add
    b = branch
    ci = commit -v
    co = checkout
    cp = cherry-pick
    h = help
    ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    ls = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short
    r = remote
    rso = remote show origin
    p = pull
    po = push origin
    pom = push origin master
    stat = status
    st = status -s

